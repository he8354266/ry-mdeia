<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.WaterfallInfoMapper">
    
    <resultMap type="WaterfallInfo" id="WaterfallInfoResult">
        <result property="id"    column="id"    />
        <result property="number"    column="number"    />
        <result property="nickName"    column="nick_name"    />
        <result property="titleImg"    column="title_img"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectWaterfallInfoVo">
        select id, number, nick_name,title_img,  create_by, create_time, update_by, update_time, remark from ry_waterfall_info
    </sql>




</mapper>